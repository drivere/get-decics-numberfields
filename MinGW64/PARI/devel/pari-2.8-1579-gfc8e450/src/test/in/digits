binary(0)
digits(0)
fromdigits([])
digits(5^100)
fromdigits(vector(100,i,i%7))
check(a,b)=my(v=digits(a,b));if(subst(Pol(v),'x,b)!=a || vecmax(v)>=b || fromdigits(v,b)!=a,error([a,b]));
check(5^100,2)
check(5^100,3)
check(3^100,2^64-1)
check(3^100,2^128+1)
sumdigits(0)
