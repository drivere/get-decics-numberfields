precision(I,3)
precision(1+O(3)+I,3)
precision(O(2),3)
precision(1+O(2),3)
precision(x+1+O(3),3)
precision((1+O(3))/((1+O(5))*x),3)
precision(Mod(1+O(3),(1+O(3))*x),3)
precision([1+O(3),2],3)
default(realprecision,38);
t=(precision(1.,77)*x+1);
precision(t)
localprec(57);precision(1.)
precision(1./t)
precision(Qfb(1,0,-2));

padicprec(1,2)  == padicprec(0,2)
padicprec(1/2,2)== padicprec(0,2)
padicprec(Mod(1,9),3)
padicprec(O(2^2),3)
padicprec(O(2^2),2)
t=1+O(2^3);
padicprec(t,2)
padicprec((x+2)*t, 2)
padicprec((1+2*x+O(x^2))*t, 2)
padicprec([2,4]*t, 2)
