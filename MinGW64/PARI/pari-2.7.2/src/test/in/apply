apply(x->x^2, [1,2,3,4])
apply(x->x^2, [1,2;3,4])
apply(x->x^2, 4*x^2 + 3*x+ 2)
apply(x->x^2, 4*x^2 + 3*x+ 2 + O(x^3))
apply(x->x^2, List([1,2,3,4]))
L = List([Mod(1,3), Mod(2,4)]); apply(lift, L)
f(x)=L=0;x-1;
L=List(); apply(f, L);
L=[1,2,3]; apply(f, L)
L=[1,2,3]; [f(x) | x<-L]
L=[1,2,3]; [x | x<-L, f(x)]
L=[1,2,3]; select(f, L)
